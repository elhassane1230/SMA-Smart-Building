                  rdfs:subClassOf :Intention ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :aPourBeneficeAdditionnel ;
                                    owl:someValuesFrom :ReductionHumidite
                                  ] ;
                  rdfs:comment "Intention d'activer le chauffage." ;
                  rdfs:label "Activer Chauffage" .


###  http://monbatiment.com/ontologie/SMA4SB#ActiverVentilation
