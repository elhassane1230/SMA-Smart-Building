[   rdfs:label "S1" ;
    rdfs:comment "Si la temperature est inferieure a 19°C, un besoin de chauffage est deduit." ;
    rdf:type swrl:Imp ;
    swrl:body
    [   rdf:type swrl:AtomList ;
        rdf:first
        [   rdf:type swrl:ClassAtom ;
            swrl:classPredicate :Piece ;
            swrl:argument1 :p
