        rdf:rest
        [   rdf:type swrl:AtomList ;
            rdf:first
            [   rdf:type swrl:ObjectPropertyAtom ;
                swrl:propertyPredicate :aPourTemperatureActuelle ;
                swrl:argument1 :p ;
                swrl:argument2 :m
            ] ;
            rdf:rest
            [   rdf:type swrl:AtomList ;
                rdf:first
                [   rdf:type swrl:DatavaluedPropertyAtom ;
                    swrl:propertyPredicate :aPourValeur ;
                    swrl:argument1 :m ;
                    swrl:argument2 :val
                ] ;
                rdf:rest
                [   rdf:type swrl:AtomList ;
                    rdf:first
                    [   rdf:type swrl:BuiltinAtom ;
                        swrl:builtin swrlb:lessThan ;
                        swrl:arguments (:val "19.0"^^xsd:decimal)
                    ] ;
                    rdf:rest rdf:nil
                ]
            ]
        ]
    ] ;
    swrl:head
    [   rdf:type swrl:AtomList ;
        rdf:first
        [   rdf:type swrl:ObjectPropertyAtom ;
            swrl:propertyPredicate :aPourBesoin ;
            swrl:argument1 :p ;
            swrl:argument2
            [   rdf:type swrl:AtomList ;
                rdf:first
                [   rdf:type swrl:ClassAtom ;
                    swrl:classPredicate :BesoinChauffage ;
                    swrl:argument1 _:b
                ] ;
                rdf:rest rdf:nil
            ]
        ] ;
        rdf:rest rdf:nil
    ]
] .

