[] a swrl:Imp ;
    rdfs:label "S2" ;
    rdfs:comment "Si le CO2 depasse 1000 ppm, un besoin de ventilation est deduit." ;
    swrl:body [ a swrl:AtomList ;
            rdf:first [ a swrl:ClassAtom ;
                    swrl:argument1 :p ;
                    swrl:classPredicate :Piece ] ;
            rdf:rest [ a swrl:AtomList ;
                    rdf:first [ a swrl:ObjectPropertyAtom ;
                            swrl:argument1 :p ;
                            swrl:argument2 :m ;
                            swrl:propertyPredicate :aPourCO2Actuel ] ;
                    rdf:rest [ a swrl:AtomList ;
                            rdf:first [ a swrl:DatavaluedPropertyAtom ;
                                    swrl:argument1 :m ;
                                    swrl:argument2 :val ;
                                    swrl:propertyPredicate :aPourValeur ] ;
                            rdf:rest [ a swrl:AtomList ;
                                    rdf:first [ a swrl:BuiltinAtom ;
                                            swrl:arguments ( :val 1000.0 ) ;
                                            swrl:builtin swrlb:greaterThan ] ;
                                    rdf:rest () ] ] ] ] ;
    swrl:head [ a swrl:AtomList ;
            rdf:first [ a swrl:ObjectPropertyAtom ;
                    swrl:argument1 :p ;
                    swrl:argument2 [ a swrl:AtomList ;
                            rdf:first [ a swrl:ClassAtom ;
                                    swrl:argument1 _:n389462b32178461fa22b6b840f65229db24 ;
                                    swrl:classPredicate :BesoinVentilation ] ;
                            rdf:rest () ] ;
                    swrl:propertyPredicate :aPourBesoin ] ;
            rdf:rest () ] .

[] a swrl:Imp ;
    rdfs:label "S3" ;
    rdfs:comment "Deux intentions visant le meme equipement avec des etats finaux differents sont en conflit." ;
    swrl:body [ a swrl:AtomList ;
            rdf:first [ a swrl:ObjectPropertyAtom ;
                    swrl:argument1 :i1 ;
                    swrl:argument2 :e ;
                    swrl:propertyPredicate :aPourCibleEquipement ] ;
            rdf:rest [ a swrl:AtomList ;
                    rdf:first [ a swrl:ObjectPropertyAtom ;
                            swrl:argument1 :i2 ;
                            swrl:argument2 :e ;
                            swrl:propertyPredicate :aPourCibleEquipement ] ;
                    rdf:rest [ a swrl:AtomList ;
                            rdf:first [ a swrl:ObjectPropertyAtom ;
                                    swrl:argument1 :i1 ;
                                    swrl:argument2 :s1 ;
                                    swrl:propertyPredicate :proposeEtatActionneur ] ;
                            rdf:rest [ a swrl:AtomList ;
                                    rdf:first [ a swrl:ObjectPropertyAtom ;
                                            swrl:argument1 :i2 ;
                                            swrl:argument2 :s2 ;
                                            swrl:propertyPredicate :proposeEtatActionneur ] ;
                                    rdf:rest [ a swrl:AtomList ;
                                            rdf:first [ a swrl:BuiltinAtom ;
                                                    swrl:arguments ( :i1 :i2 ) ;
                                                    swrl:builtin swrlb:notEqual ] ;
                                            rdf:rest [ a swrl:AtomList ;
                                                    rdf:first [ a swrl:BuiltinAtom ;
                                                            swrl:arguments ( :s1 :s2 ) ;
                                                            swrl:builtin swrlb:notEqual ] ;
                                                    rdf:rest () ] ] ] ] ] ] ;
    swrl:head [ a swrl:AtomList ;
            rdf:first [ a swrl:ObjectPropertyAtom ;
                    swrl:argument1 :i1 ;
                    swrl:argument2 :i2 ;
                    swrl:propertyPredicate :estEnConflitAvec ] ;
            rdf:rest () ] .

